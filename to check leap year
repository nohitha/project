#include<stdio.h>
void main()
{
int y;
printf("enter any year\n");
scanf("%d",&y);
if(y%4==0)
{
 if(y%100==0)
  {
   if(y%400==0)
   printf("\n %d is a leap year",y);
   else
   printf("\n %d is not a leap year",y)
  }
  else
   printf("\n %d is a leap year",y);
}
else
printf("\n %d is not a leap year",y);
}
